HALT	0000	R
NOP		0001	R
WRITE	0011	R
FETCH	0100	R
ADD		0101	R
NAND	0110	R
READ	0111	R
DATA	1000	R
ACAD	1001	R
LITAD	1010	R*
STORE	1011	R
LOAD	1100	J
BOF		1101	J
BEQ		1110	J
JMP		1111	J

label $reg0 = x0000
label $reg1 = x0001
label $reg2 = x0002
label $reg3 = x0003
label $reg4 = x0004
label $reg5 = x0005
label $reg6 = x0006
label $reg7 = x0007
